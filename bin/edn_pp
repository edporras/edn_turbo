#!/usr/bin/env ruby
# -*- coding: utf-8 -*-

$LOAD_PATH << File.expand_path( File.dirname(__FILE__) + '/../lib' )
require 'edn_ext'
require 'pp'

if ARGV.size == 0
  $stderr.puts "invalid arg count"
  exit 1
end

filename = ARGV[0]
if !File.exist? filename
  $stderr.puts "Path '#{filename}' not valid"
  exit 1
end

pp EDN_EXT.read(filename)
